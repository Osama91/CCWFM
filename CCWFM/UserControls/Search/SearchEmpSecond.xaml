<UserControl x:Class="CCWFM.UserControls.Search.SearchEmpSecond"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="330" d:DesignWidth="400">
    <!--<StackPanel x:Name="LayoutRoot" Orientation="Vertical" Height="330" >-->
        <StackPanel  Orientation="Horizontal" Background="White">
            <TextBlock VerticalAlignment="Center" Margin="5" Text="{Binding SecondEmpPerRow.Emplid,Mode=TwoWay}"></TextBlock>
            <TextBlock VerticalAlignment="Center" Margin="5" Text="{Binding SecondEmpPerRow.Name,Mode=TwoWay}"></TextBlock>
            <Button  Name="BtnSearch" Click="BtnSearch_OnClick">
                <Image HorizontalAlignment="Stretch"  Width="30" Height="30"
                       VerticalAlignment="Stretch"
                       Source="/CCWFM;component/Images/Search.png"
                       Stretch="Uniform" />
            </Button>
        </StackPanel>
        
        <!--<TextBox Height="30" Text="{Binding SecondEmpPerRow.Name, Mode=TwoWay}" x:Name="txtUserName"/>-->
    <!--</StackPanel>-->
   
</UserControl>