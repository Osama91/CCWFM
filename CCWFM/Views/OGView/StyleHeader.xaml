<navigation:Page x:Class="CCWFM.Views.OGView.StyleHeader"
				 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				 xmlns:converters="clr-namespace:CCWFM.Helpers.Converters"
				 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
				 xmlns:dataGrid="clr-namespace:Os.Controls.DataGrid;assembly=Os.Controls"
				 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
				 xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
				 xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
				 xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
				 xmlns:userControls="clr-namespace:CCWFM.UserControls"
				 xmlns:search="clr-namespace:CCWFM.UserControls.Search"
                 xmlns:datagridExtenders="clr-namespace:CCWFM.Helpers.ControlsExtenders.DatagridExtenders"
				 xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
                 xmlns:syncfusion1="clr-namespace:Syncfusion.Windows.Tools.Controls;assembly=Syncfusion.Shared.Silverlight"
				 xmlns:globalExtenders="clr-namespace:CCWFM.Helpers.ControlsExtenders.GlobalExtenders"
                 xmlns:cmd="http://www.galasoft.ch/mvvmlight"
				 xmlns:utilities="clr-namespace:CCWFM.Helpers.Utilities" 
                 Title="StyleHeader"
				 d:DesignHeight="480" 
                 d:DesignWidth="2500" 
                 mc:Ignorable="d">
    <navigation:Page.Resources>
        <converters:IntToEnabledConvertor x:Key="IntToEnabledConvertor" />
    </navigation:Page.Resources>
    <Grid x:Name="LayoutRoot" 
          KeyDown="LayoutRoot_OnKeyDown">
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".90*" />
            <ColumnDefinition Width=".10*" />
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Row="0"
					Grid.Column="0"
					HorizontalAlignment="Stretch"
					Orientation="Horizontal">
            <StackPanel.Background>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,9">
                    <GradientStop Color="#FFC6ECFE" />
                    <GradientStop Offset=".01" Color="#FF003FFF" />
                    <GradientStop Offset="0.3" Color="#ff02fdce" />
                </LinearGradientBrush>
            </StackPanel.Background>
            <Button Name="BtnAddNewMainRow"
					HorizontalAlignment="Stretch"
					VerticalAlignment="Stretch"
					Click="BtnAddNewMainRow_Click"
					Cursor="Hand"
					ToolTipService.ToolTip="Add">
                <Image Width="30"
					   HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch"
					   Source="/CCWFM;component/Images/1371054818_New_File.png"
					   Stretch="Uniform" />
            </Button>
            <Button Name="BtnSave"
					Click="BtnSave_Onclick"
					Content="Save" />
            <Button Name="BtnImport"
					Click="BtnImport_Click"
					Content="Import" />
            <Button Name="BtnExportExcel"
					HorizontalAlignment="Stretch"
					VerticalAlignment="Stretch"
					Click="ExportExcel_Click"
					Cursor="Hand"
					ToolTipService.ToolTip="Export">
                <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch"
					   Source="/CCWFM;component/Images/ExportExcel.png"
					   Stretch="Uniform" />
            </Button>
            <Button Name="BtnPrintPreviewOrder"
					HorizontalAlignment="Stretch"
					VerticalAlignment="Stretch"
					Click="btnPrintPreviewOrder_Click"
					ToolTipService.ToolTip="Print">
                <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch"
					   Source="/CCWFM;component/Images/1372257722_MB__print.png"
					   Stretch="Uniform" />
            </Button>
            <Button Name="BtnGetPending"
					Click="BtnGetPending_OnClick"
					Content="Pending" />
            <Button Name="BtnGetApproved"
					Click="BtnGetApproved_OnClick"
					Content="Approved" />
            <Button Name="BtnCopyProduct"
					Click="BtnCopyProduct_OnClick"
					Content="Copy Product" Height="40" VerticalAlignment="Top" />
            <Button x:Name="btnGridColumnOrder" Content="Columns"
                    Click="btnGridColumnOrder_Click" HorizontalAlignment="Left"
                    Height="40" VerticalAlignment="Top" Width="75"></Button>
        </StackPanel>
        <Grid Grid.Row="0" 
			  Grid.Column="0"
			  HorizontalAlignment="Right"
			  VerticalAlignment="Center">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <TextBlock Width="200" 
					   Foreground="White"
					   Text="{Binding SelectedMainRow.StyleCode}" />
            <TextBlock Grid.Column="1"
					   Width="200"
					   Foreground="White"
					   Text="{Binding SelectedDetailRow.SalesOrderCode}" />
        </Grid>
        <Border Grid.Row="0"
				Grid.RowSpan="2"
				Grid.Column="1"
				BorderBrush="DarkGray"
				BorderThickness="2,2,2,2">
            <Image HorizontalAlignment="Stretch" Name="imgStylePic" Cursor="Hand" ToolTipService.ToolTip="Show Image" 
				   Source="{Binding SelectedMainRow.StyleImage,
									Converter={StaticResource ByteToImageConverter},
									Mode=TwoWay}"
				   VerticalAlignment="Stretch"
                   
				   Stretch="Uniform" MouseLeftButtonUp="imgStylePic_MouseLeftButtonUp" />
        </Border>
        <Grid Name="GridTna" Visibility="Collapsed" Grid.Row="1" MaxHeight="30" HorizontalAlignment="Left">
            <StackPanel Name="BtnsStackPanel" Orientation="Horizontal">
                <Button Tag="" Name="BtnAll" Visibility="Collapsed" Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center">All</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                    </StackPanel>
                </Button>
                <Button Tag="RequireTna" Name="BtnRequireTna"  Visibility="Collapsed" Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" >Require Tna</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.RequireTna,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
                <Button Tag="RequireTnaModification" Name="BtnRequireTnaModification"  Visibility="Collapsed" Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" >Require TNA Modification</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.RequireTNAModification,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
                <Button Tag="Modified" Name="BtnModified"  Visibility="Collapsed" Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" >Modified</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.Modified,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
                <Button  Tag="TNARoute" Name="BtnTNARoute"  Visibility="Collapsed" Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" >TNA Route</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.TNARoute,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
                <Button  Tag="RequirePo" Name="BtnRequirePo"  Visibility="Collapsed" Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" >Require Po</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.RequireSo,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
                <Button  Tag="RequirePoCost" Name="BtnRequirePoCost"  Visibility="Collapsed"  Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center" >Require Po Cost</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.RequireSoCost,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
                <Button Tag="RequirePoApproval" Name="BtnRequirePoApproval"    Click="BtnTnaCount_Click" >
                    <StackPanel Orientation="Horizontal">
                        <TextBlock VerticalAlignment="Center">TNA Approved</TextBlock>
                        <Image HorizontalAlignment="Stretch"
					   VerticalAlignment="Stretch" 
					   Source="/CCWFM;component/assets/Images/Arrow.png"
					   Stretch="Fill" />
                        <TextBlock Foreground="DarkGreen" VerticalAlignment="Center" Text="{Binding StyleTnaPerRow.RequireSoApproval,Mode=TwoWay}" ></TextBlock>
                    </StackPanel>
                </Button>
            </StackPanel>
        </Grid>
        <Grid Grid.Row="2"
			  Grid.Column="0"
			  Grid.ColumnSpan="2" Margin="0,0,0,-40">
            <sdk:TabControl Name="TabStyle"
							HorizontalAlignment="Stretch"
							VerticalAlignment="Top"
							SelectionChanged="TabStyle_SelectionChanged" 
                            Margin="10,0,31,0" Height="420">
                <sdk:TabItem Name="TabStyleItem" Header="{Binding StyleList, Source={StaticResource LocalizedStrings}}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="85*"/>
                            <ColumnDefinition Width="2362*"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel Orientation="Vertical" Visibility="Visible" Grid.ColumnSpan="2">
                            <StackPanel Orientation="Horizontal" Margin="0,5,0,2">
                                <sdk:Label Margin="0,0,5,0" Content="{Binding Brand, Source={StaticResource LocalizedStrings}}"/>
                                <ComboBox x:Name="comBrandFilter"
												  Width="150"
												  Height="23"
												  Margin="3"
												  HorizontalAlignment="Left"
												  VerticalAlignment="Center"
												  DisplayMemberPath="Brand_Ename"
                                                  SelectionChanged="comBrandFilter_SelectionChanged"
                                                  ItemsSource="{Binding BrandFilterList}"
												  SelectedValuePath="Brand_Code" />
                                <sdk:Label Margin="0,0,5,0" Content="Section"/>
                                <syncfusion1:ComboBoxAdv x:Name="comBrandSectionFilter" 
                                                         SelectionChanged="comBrandSectionFilter_SelectionChanged"   
                                           ItemsSource="{Binding BrandSectionFilterList, Mode=TwoWay}"
                                 SelectedItems="{Binding SelectedBrandSectionFilterList, Mode=TwoWay}"
                                 DisplayMemberPath="Ename" SelectedValuePath="Iserial"
                                                         AllowMultiSelect="True" Height="28" Width="150" >
                                </syncfusion1:ComboBoxAdv>
                                <sdk:Label Margin="5,0,2,0" Content="Direction"/>
                                <syncfusion1:ComboBoxAdv x:Name="comDirectionFilter"   ItemsSource="{Binding DirectionFilterList, Mode=TwoWay}"
                                 SelectedItems="{Binding SelectedDirectionFilterList, Mode=TwoWay}"
                                 DisplayMemberPath="Ename" SelectedValuePath="Iserial" SelectionChanged="comDirectionFilter_SelectionChanged"
                                                           AllowMultiSelect="True" Height="28" Width="150" >
                                </syncfusion1:ComboBoxAdv>
                                <sdk:Label Margin="5,0,2,0" Content="Category"/>
                                <syncfusion1:ComboBoxAdv  x:Name="comStyleCategoryFilter"  ItemsSource="{Binding StyleCategoryFilterList, Mode=TwoWay}"
                                 SelectedItems="{Binding SelectedStyleCategoryFilterList, Mode=TwoWay}"
                                 DisplayMemberPath="Ename" SelectedValuePath="Iserial" SelectionChanged="comStyleCategoryFilter_SelectionChanged"
                                                           AllowMultiSelect="True" Height="28" Width="150" >
                                </syncfusion1:ComboBoxAdv>
                                <sdk:Label Margin="5,0,2,0" Content="Family"/>
                                <syncfusion1:ComboBoxAdv x:Name="comFamilyFilter"  ItemsSource="{Binding FamilyFilterList, Mode=TwoWay}"
                                 SelectedItems="{Binding SelectedFamilyFilterList, Mode=TwoWay}" SelectionChanged="comFamilyFilter_SelectionChanged"
                                 DisplayMemberPath="Ename" SelectedValuePath="Iserial" 
                                                           AllowMultiSelect="True" Height="28" Width="159" >
                                </syncfusion1:ComboBoxAdv>
                                <sdk:Label Margin="5,0,2,0" Content="SubFamily"/>
                                <syncfusion1:ComboBoxAdv   ItemsSource="{Binding SubFamilyFilterList, Mode=TwoWay}" x:Name="comSubFamilyFilter"
                                 SelectedItems="{Binding SelectedSubFamilyFilterList, Mode=TwoWay}" SelectionChanged="comSubFamilyFilter_SelectionChanged"
                                 DisplayMemberPath="Ename" SelectedValuePath="Iserial" 
                                                           AllowMultiSelect="True" Height="28" Width="150" >
                                </syncfusion1:ComboBoxAdv>
                                <Button Name="BtnClearFilter" Margin="5,0,0,0" Click="BtnClearFilter_Click"  ToolTipService.ToolTip="Clear Filter">
                                    <Image Source="/CCWFM;component/Images/ImageListIcons/delete.png" Height="28" Width="30" Stretch="Uniform" />
                                </Button>
                            </StackPanel>
                        </StackPanel>
                        <dataGrid:OsGrid Name="MainGrid" 
									 Height="333"
									 Margin="0,40,20,0"
									 HorizontalContentAlignment="Stretch"
									 VerticalContentAlignment="Stretch"
									 AutoGenerateColumns="False"
                                     VerticalScrollBarVisibility="Auto"
									 FilteredItemsSource="{Binding MainRowList,
																   Mode=TwoWay}"
									 HorizontalScrollBarVisibility="Visible"
									 IsReadOnly="True"
									 ItemsSource="{Binding MainRowList,
														   Mode=TwoWay}"
									 KeyUp="MainGrid_KeyUp"
									 LoadingRow="MainGrid_LoadingRow"
									 OnFilter="MainGrid_OnFilter"
									 RowEditEnded="MainGrid_RowEditEnded"
									 SelectedItem="{Binding SelectedMainRow,
															Mode=TwoWay}"						                                     
                                     SelectionChanged="MainGrid_SelectionChanged" Grid.ColumnSpan="2">
                            <sdk:DataGrid.RowStyle>
                                <Style TargetType="sdk:DataGridRow">

                                    <Setter Property="IsTabStop" Value="False" />

                                    <Setter Property="Template">

                                        <Setter.Value>

                                            <ControlTemplate TargetType="sdk:DataGridRow" xmlns:localprimitives="clr-namespace:System.Windows.Controls.Primitives;assembly=System.Windows.Controls.Data"
             xmlns:local="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data" >

                                                <localprimitives:DataGridFrozenGrid Name="Root">

                                                    <VisualStateManager.VisualStateGroups>

                                                        <VisualStateGroup x:Name="CommonStates">

                                                            <VisualStateGroup.Transitions>

                                                                <VisualTransition GeneratedDuration="0" />
                                                            </VisualStateGroup.Transitions>

                                                            <VisualState x:Name="Normal" >

                                                            </VisualState>

                                                            <VisualState x:Name="Normal AlternatingRow">

                                                                <Storyboard>

                                                                    <DoubleAnimation Storyboard.TargetName="BackgroundRectangle" Storyboard.TargetProperty="Opacity" Duration="0" To="0" />
                                                                </Storyboard>
                                                            </VisualState>

                                                            <VisualState x:Name="MouseOver">

                                                                <Storyboard>

                                                                    <DoubleAnimation Storyboard.TargetName="BackgroundRectangle" Storyboard.TargetProperty="Opacity" Duration="0" To="0" />
                                                                </Storyboard>
                                                            </VisualState>

                                                            <VisualState x:Name="Normal Selected">

                                                                <Storyboard>

                                                                    <DoubleAnimation Storyboard.TargetName="BackgroundRectangle" Storyboard.TargetProperty="Opacity" Duration="0" To="1" />

                                                                </Storyboard>
                                                            </VisualState>

                                                            <VisualState x:Name="MouseOver Selected">

                                                                <Storyboard>

                                                                    <DoubleAnimation Storyboard.TargetName="BackgroundRectangle" Storyboard.TargetProperty="Opacity" Duration="0" To="1" />

                                                                </Storyboard>
                                                            </VisualState>

                                                            <VisualState x:Name="Unfocused Selected">

                                                                <Storyboard>

                                                                    <DoubleAnimation Storyboard.TargetName="BackgroundRectangle" Storyboard.TargetProperty="Opacity" Duration="0" To="1" />

                                                                    <ColorAnimationUsingKeyFrames BeginTime="0" Duration="0" Storyboard.TargetName="BackgroundRectangle"
                                                                      Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)">

                                                                        <SplineColorKeyFrame KeyTime="0" Value="#874385f6" />
                                                                    </ColorAnimationUsingKeyFrames>

                                                                </Storyboard>
                                                            </VisualState>
                                                        </VisualStateGroup>
                                                    </VisualStateManager.VisualStateGroups>

                                                    <Grid.RowDefinitions>

                                                        <RowDefinition Height="*" />

                                                        <RowDefinition Height="Auto" />

                                                        <RowDefinition Height="Auto" />
                                                    </Grid.RowDefinitions>

                                                    <Grid.ColumnDefinitions>

                                                        <ColumnDefinition Width="Auto" />

                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>

                                                    <Grid.Resources>

                                                        <Storyboard x:Key="DetailsVisibleTransition">

                                                            <DoubleAnimation Storyboard.TargetName="DetailsPresenter" Storyboard.TargetProperty="ContentHeight" Duration="00:00:0.1" />
                                                        </Storyboard>
                                                    </Grid.Resources>

                                                    <Rectangle x:Name="BackgroundRectangle" Grid.RowSpan="2" Grid.ColumnSpan="2" Opacity="0" Fill="#ff4385f6" />

                                                    <localprimitives:DataGridRowHeader Grid.RowSpan="3" Name="RowHeader" localprimitives:DataGridFrozenGrid.IsFrozen="True" />

                                                    <localprimitives:DataGridCellsPresenter Grid.Column="1" Name="CellsPresenter" Background="{Binding Approved}" localprimitives:DataGridFrozenGrid.IsFrozen="True" />

                                                    <localprimitives:DataGridDetailsPresenter Grid.Row="1" Grid.Column="1" Name="DetailsPresenter" />

                                                    <Rectangle Grid.Row="2" Grid.Column="1" Name="BottomGridLine" HorizontalAlignment="Stretch" Height="1" />
                                                </localprimitives:DataGridFrozenGrid>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>

                            </sdk:DataGrid.RowStyle>
